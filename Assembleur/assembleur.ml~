let labels = Hashtbl.create 17 

let int_to_string i = 
	let s = String.create 8 in 
	for i = 0 to 7 do 
	  s.[i] <- if (i/(1 lsl i)) mod 2 = 0 then '0' else '1' ;
	done ;
	s

