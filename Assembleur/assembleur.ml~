let rec get_lab i lexbuf =
  lexbuf.Lexing.lex_mem <- Array.create 4 (-1) ; (* L=1 [2] <- p ;  *)
  lexbuf.Lexing.lex_mem.(2) <- lexbuf.Lexing.lex_curr_pos ;
  __ocaml_lex_get_lab_rec i lexbuf 0
and __ocaml_lex_get_lab_rec i lexbuf __ocaml_lex_state =
  match Lexing.new_engine __ocaml_lex_tables __ocaml_lex_state lexbuf with
      | 0 ->
let
# 72 "assembleur.mll"
                  lab
# 627 "assembleur.ml"
= Lexing.sub_lexeme lexbuf lexbuf.Lexing.lex_mem.(0) lexbuf.Lexing.lex_mem.(1) in
# 72 "assembleur.mll"
                                     (if Hashtbl.mem labels lab 
			then (raise (Assembleur_error "label defined twice"))
			else Hashtbl.add labels lab (int_to_string i))
# 633 "assembleur.ml"

  | 1 ->
# 75 "assembleur.mll"
        (get_lab i lexbuf)
# 638 "assembleur.ml"

  | 2 ->
# 76 "assembleur.mll"
       ()
# 643 "assembleur.ml"

  | 3 ->
# 77 "assembleur.mll"
        (comment lexbuf ; get_lab i lexbuf)
# 648 "assembleur.ml"

  | 4 ->
# 78 "assembleur.mll"
                 (get_lab (i+1) lexbuf)
# 653 "assembleur.ml"

  | __ocaml_lex_state -> lexbuf.Lexing.refill_buff lexbuf; __ocaml_lex_get_lab_rec i lexbuf __ocaml_lex_state

and assembleur fft lexbuf =
  lexbuf.Lexing.lex_mem <- Array.create 12 (-1) ;   __ocaml_lex_assembleur_rec fft lexbuf 11
and __ocaml_lex_assembleur_rec fft lexbuf __ocaml_lex_state =
  match Lexing.new_engine __ocaml_lex_tables __ocaml_lex_state lexbuf with
      | 0 ->
let
# 81 "assembleur.mll"
             op
# 665 "assembleur.ml"
= Lexing.sub_lexeme lexbuf lexbuf.Lexing.lex_start_pos lexbuf.Lexing.lex_mem.(0)
and
# 81 "assembleur.mll"
                                    reg1
# 670 "assembleur.ml"
= Lexing.sub_lexeme lexbuf lexbuf.Lexing.lex_mem.(1) lexbuf.Lexing.lex_mem.(2)
and
# 81 "assembleur.mll"
                                                         reg2
# 675 "assembleur.ml"
= Lexing.sub_lexeme lexbuf lexbuf.Lexing.lex_mem.(3) lexbuf.Lexing.lex_curr_pos in
# 82 "assembleur.mll"
 (Format.fprintf fft "%s%s%s\n" (Hashtbl.find key_words op) (Hashtbl.find registres reg1) (Hashtbl.find registres reg2) ;
	assembleur fft lexbuf)
# 680 "assembleur.ml"

  | 1 ->
let
# 84 "assembleur.mll"
             op
# 686 "assembleur.ml"
= Lexing.sub_lexeme lexbuf lexbuf.Lexing.lex_start_pos lexbuf.Lexing.lex_mem.(0)
and
# 84 "assembleur.mll"
                                lab
# 691 "assembleur.ml"
= Lexing.sub_lexeme lexbuf lexbuf.Lexing.lex_mem.(1) lexbuf.Lexing.lex_curr_pos in
# 84 "assembleur.mll"
                                     (Format.fprintf fft "%s%s\n" (Hashtbl.find key_words op) (Hashtbl.find labels lab))
# 695 "assembleur.ml"

  | 2 ->
let
# 85 "assembleur.mll"
             op
# 701 "assembleur.ml"
= Lexing.sub_lexeme lexbuf lexbuf.Lexing.lex_start_pos lexbuf.Lexing.lex_mem.(0)
and
# 85 "assembleur.mll"
                                   i
# 706 "assembleur.ml"
= Lexing.sub_lexeme lexbuf lexbuf.Lexing.lex_mem.(1) lexbuf.Lexing.lex_curr_pos in
# 85 "assembleur.mll"
                                      (Format.fprintf fft "%s%s\n" (Hashtbl.find key_words op) (int_to_string i))
# 710 "assembleur.ml"

  | 3 ->
# 86 "assembleur.mll"
        (comment lexbuf)
# 715 "assembleur.ml"

  | 4 ->
# 87 "assembleur.mll"
       ()
# 720 "assembleur.ml"

  | __ocaml_lex_state -> lexbuf.Lexing.refill_buff lexbuf; __ocaml_lex_assembleur_rec fft lexbuf __ocaml_lex_state

and comment lexbuf =
    __ocaml_lex_comment_rec lexbuf 23
and __ocaml_lex_comment_rec lexbuf __ocaml_lex_state =
  match Lexing.engine __ocaml_lex_tables __ocaml_lex_state lexbuf with
      | 0 ->
# 91 "assembleur.mll"
        ()
# 731 "assembleur.ml"

  | 1 ->
# 92 "assembleur.mll"
        (comment lexbuf ; comment lexbuf)
# 736 "assembleur.ml"

  | 2 ->
# 93 "assembleur.mll"
       (raise (Assembleur_error "unfinished comment"))
# 741 "assembleur.ml"

  | 3 ->
# 94 "assembleur.mll"
     (comment lexbuf)
# 746 "assembleur.ml"

  | __ocaml_lex_state -> lexbuf.Lexing.refill_buff lexbuf; __ocaml_lex_comment_rec lexbuf __ocaml_lex_state

;;

# 95 "assembleur.mll"
 

let usage = "usage : assembleur file"
let ifile = ref ""
let set_file f s = f := s ;

let () = Arg.parse [] (set_file ifile) usage in 
 if !ifile="" then begin eprintf "Aucun fichier à compiler\n@?"; exit 1 end; 

  if not (Filename.check_suffix !ifile ".asm") then begin
    eprintf "Le fichier d'entrée doit avoir l'extension .asm\n@?";
    Arg.usage options usage;
    exit 1
  end;
  ofile := (Filename.chop_suffix !ifile ".asm") ^ ".rom" ;

 let f = open_in !ifile in
  let buf = Lexing.from_channel f in
  get_lab 0 buf ;
  Lexing.flush_input buf ;
  let output = open_out !ofile
  out_fmt = Format.formatter_of_out_channel output ;
  assembleur out_fmt buf ;
  close_in !f ;
  close_out output


# 780 "assembleur.ml"
